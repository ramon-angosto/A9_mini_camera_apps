<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:pangle="http://schemas.android.com/apk/com.bytedance.sdk.component" xmlns:aapt="http://schemas.android.com/aapt" android:layout_width="match_parent" android:layout_height="match_parent">
    <LinearLayout android:orientation="vertical" android:layout_width="match_parent" android:layout_height="match_parent">
        <ViewStub android:id="@+id/tt_browser_titlebar_view_stub" android:layout="@layout/tt_browser_titlebar" android:layout_width="match_parent" android:layout_height="44dp"/>
        <ViewStub android:id="@+id/tt_browser_titlebar_dark_view_stub" android:layout="@layout/tt_browser_titlebar_for_dark" android:layout_width="match_parent" android:layout_height="44dp"/>
        <ViewStub android:id="@+id/tt_browser_new_title_bar_view_stub" android:layout="@layout/tt_browser_new_title_bar" android:layout_width="match_parent" android:layout_height="44dp"/>
        <FrameLayout android:layout_width="match_parent" android:layout_height="0dp" android:layout_weight="1">
            <com.bytedance.sdk.component.widget.SSWebView android:id="@+id/tt_browser_webview" android:layout_width="match_parent" android:layout_height="match_parent"/>
            <ViewStub android:layout_gravity="bottom|center_vertical|center_horizontal|center" android:id="@+id/tt_browser_download_btn_stub" android:layout="@layout/tt_browser_download_layout" android:layout_width="match_parent" android:layout_height="wrap_content"/>
            <ProgressBar android:layout_gravity="top|center_vertical|center_horizontal|center" android:id="@+id/tt_browser_progress" android:visibility="gone" android:layout_width="match_parent" android:layout_height="3dp" android:progress="1" android:progressDrawable="@drawable/tt_browser_progress_style" style="@style/Widget.ProgressBar.Horizontal"/>
            <ViewStub android:layout_gravity="bottom" android:id="@+id/tt_browser_new_bottom_bar_view_stub" android:layout="@layout/tt_browser_new_bottom_bar" android:layout_width="match_parent" android:layout_height="44dp"/>
        </FrameLayout>
    </LinearLayout>
    <com.bytedance.sdk.openadsdk.common.LandingPageLoadingLayout android:id="@+id/tt_landing_page_loading_layout" android:layout_width="match_parent" android:layout_height="match_parent"/>
</FrameLayout>
