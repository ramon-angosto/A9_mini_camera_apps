<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_pressed="true">
        <shape>
            <stroke android:width="1dp" android:color="#1f000000"/>
            <solid android:color="#0d000000"/>
            <corners android:radius="2dp"/>
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape>
            <stroke android:width="1dp" android:color="#1618231f"/>
            <solid android:color="#ffffff"/>
            <corners android:radius="2dp"/>
        </shape>
    </item>
</selector>
