<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="25dp"/>
            <gradient android:startColor="#8fc31f" android:endColor="#8fc31f" android:angle="0"/>
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="25dp"/>
                <gradient android:startColor="#73a210" android:endColor="#73a210" android:angle="0"/>
            </shape>
        </clip>
    </item>
</layer-list>
